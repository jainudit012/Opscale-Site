    <div class="nav__spot"></div>
    <section class="page-404 hidden" id="project-404">
      <h2>The project is not available!</h2>
    </section>
    <main class="project__section__wrapper" id="projects__wrapper">
    <%_ _projects.forEach((projectData, index) =>{ -%>
      <section class="project__section <%_ if(index === 0){ %> block <%_ }_%>" id="project-<%=index + 1%>" data-projectNumber="<%=index + 1%>" data-industry="<%=projectData.tag%>">
        <div class="project__mast__wrapper" style="<%= projectData.mastImageStyle %>">
            <div class="project__name__box">
                <h4 class="project__client-name"><%= projectData.clientName %></h4>
                <p class="project__industry-name"><%= projectData.industry %></p>
                <div class="project__name--pill"></div>
            </div>
            <h4 class="project__info-detail">Project Heading : <%= projectData.heading %></h4>
            <div class="sections__button-group">
                <a class="sections__button-group__link" href="#more-<%=index + 1%>">
                    <img src="./assets/downArrow.svg" alt="More" class="sections__arrow-down">
                    <span class="sections__learn">Learn More</span>
                </a>
            </div>
        </div>
        <div class="project__background-wrapper" id="more-<%=index + 1%>">
            <div class="project__background-text-box">
                <h3 class="project__background-heading">Background</h3>
                <p class="project__background-text"><%= projectData.background %></p>
            </div>
            <div class="project__background-img-box">
                <img src="https://res.cloudinary.com/opscale/image/upload/q_auto/v1586871687/phone_metrics_llksbj.jpg" srcset="https://res.cloudinary.com/opscale/image/upload/q_auto/v1586871687/phone_metrics_llksbj.jpg 1x, https://res.cloudinary.com/opscale/image/upload/q_auto/v1586871687/phone_metrics_2x_sseffg.jpg 2x" alt="phone" class="project__background-img">
            </div>
        </div>
        <div class="project__challenges-wrapper">
            <h3 class="project__challenges-heading">Challenges</h3>
            <p class="project__challenges-text"><%= projectData.challenges.info %></p>
            <ul class="project__challenges-list-wrapper">
            <%_ projectData.challenges.steps.forEach((step, stepIndex) =>{ -%>
              <li class="project__challenges-list-item">
                <span class="project__challenges-list-number">0<%=stepIndex + 1%></span>
                <span class="project__challenges-list-text"><%= step %></span>
              </li>
            <%_ }) _%>
            </ul>
        </div>
        <div class="project__approach-wrapper">
            <h3 class="project__approach-heading">Approach</h3>
            <div class="project__approach__cards-box">
                <div class="project__approach__info__card">
                    <header class="project__approach__info__card--header">
                      <img src="./assets/medal.svg" alt="Medal" class="project__approach__info__card--header-img">
                    </header>
                    <h3 class="project__approach__info__card--bold"><%= projectData.approach[0].heading %></h3>
                    <p class="project__approach__info__card--text"><%= projectData.approach[0].text %></p>
                </div>
                <div class="project__approach__info__card">
                    <header class="project__approach__info__card--header">
                        <img src="./assets/team.svg" alt="Team" class="project__approach__info__card--header-img">
                    </header>
                    <h3 class="project__approach__info__card--bold"><%= projectData.approach[1].heading %></h3>
                    <p class="project__approach__info__card--text"><%= projectData.approach[1].text %></p>
                </div>
                <div class="project__approach__info__card">
                    <header class="project__approach__info__card--header">
                        <img src="./assets/handshake.svg" alt="Handshake" class="project__approach__info__card--header-img">
                    </header>
                    <h3 class="project__approach__info__card--bold"><%= projectData.approach[2].heading %></h3>
                    <p class="project__approach__info__card--text"><%= projectData.approach[2].text %></p>
                </div>
            </div>
        </div>
        <div class="project__solution-wrapper">
            <div class="project__solution-info">
                <h3 class="project__solution-heading">Solution</h3>
              <%_ projectData.solution.forEach(solution =>{ -%>
                <p class="project__solution-text"><%= solution %></p>
              <%_ }) _%>
            </div>
            <div class="project__solution-img-box">
                <img src="https://res.cloudinary.com/opscale/image/upload/q_auto/v1586871687/phone_other_ixjh3r.jpg" srcset="https://res.cloudinary.com/opscale/image/upload/q_auto/v1586871687/phone_other_ixjh3r.jpg 1x, https://res.cloudinary.com/opscale/image/upload/q_auto/v1586871687/phone_other_2x_kkd6de.jpg 2x" alt="phone" class="project__solution-img">
            </div>
        </div>
        <div class="project__technology-wrapper">
            <h4 class="project__technology-heading">Technology</h4>
            <div class="project__technology-img-wrapper">
            <% if(projectData.technologies.indexOf('redux') > -1) { _%> <img src="https://res.cloudinary.com/opscale/image/upload/q_auto/v1586871688/redux-saga_vm27da.jpg" srcset="https://res.cloudinary.com/opscale/image/upload/q_auto/v1586871688/redux-saga_vm27da.jpg 1x, https://res.cloudinary.com/opscale/image/upload/q_auto/v1586871689/redux-saga_2x_ajudpo.jpg 2x" alt="Redux" class="project__technology-img"> <%_ }_%>
            <% if(projectData.technologies.indexOf('java') > -1) { _%> <img src="https://res.cloudinary.com/opscale/image/upload/q_auto/v1586871685/java_j6xsjd.jpg" srcset="https://res.cloudinary.com/opscale/image/upload/q_auto/v1586871685/java_j6xsjd.jpg 1x, https://res.cloudinary.com/opscale/image/upload/q_auto/v1586871685/java_2x_qghktf.jpg 2x" alt="Java" class="project__technology-img"> <%_ }_%>
            <% if(projectData.technologies.indexOf('react') > -1) { _%> <img src="https://res.cloudinary.com/opscale/image/upload/q_auto/v1586871688/react-icon_of2mok.jpg" srcset="https://res.cloudinary.com/opscale/image/upload/q_auto/v1586871688/react-icon_of2mok.jpg 1x, https://res.cloudinary.com/opscale/image/upload/q_auto/v1586871688/react-icon_2x_sc7cw4.jpg 2x" alt="React JS" class="project__technology-img"> <%_ }_%>
            <% if(projectData.technologies.indexOf('spring') > -1) { _%> <img src="https://res.cloudinary.com/opscale/image/upload/q_auto/v1586871689/spring-logo_xjpuwy.jpg" srcset="https://res.cloudinary.com/opscale/image/upload/q_auto/v1586871689/spring-logo_xjpuwy.jpg 1x, https://res.cloudinary.com/opscale/image/upload/q_auto/v1586871689/spring-logo_2x_gekaze.jpg 2x" alt="Spring" class="project__technology-img"> <%_ }_%>
            <% if(projectData.technologies.indexOf('node') > -1) { _%> <img src="https://res.cloudinary.com/opscale/image/upload/q_auto/v1586871686/node-logo_nssmdw.jpg" srcset="https://res.cloudinary.com/opscale/image/upload/q_auto/v1586871686/node-logo_nssmdw.jpg 1x, https://res.cloudinary.com/opscale/image/upload/q_auto/v1586871686/node-logo_2x_f02jf7.jpg 2x" alt="Node JS" class="project__technology-img"> <%_ }_%>
            <% if(projectData.technologies.indexOf('python') > -1) { _%> <img src="https://res.cloudinary.com/opscale/image/upload/q_auto/v1586871688/python_wsbvjb.jpg" srcset="https://res.cloudinary.com/opscale/image/upload/q_auto/v1586871688/python_wsbvjb.jpg 1x, https://res.cloudinary.com/opscale/image/upload/q_auto/v1586871688/python_2x_djr6vr.jpg 2x" alt="Python" class="project__technology-img"> <%_ }_%>
            <% if(projectData.technologies.indexOf('postgres') > -1) { _%> <img src="https://res.cloudinary.com/opscale/image/upload/q_auto/v1586871688/psql-logo_l4xv4h.jpg" srcset="https://res.cloudinary.com/opscale/image/upload/q_auto/v1586871688/psql-logo_l4xv4h.jpg 1x, https://res.cloudinary.com/opscale/image/upload/q_auto/v1586871688/psql-logo_2x_qqna4v.jpg 2x" alt="Postgres SQL" class="project__technology-img"> <%_ }_%>
            </div>
        </div>
        <div class="project__technology__stories">
          <header class="project__technology__stories-header">
            <img src="<%= projectData.ssHeaderImageUrl %>" srcset="<%= projectData.ssHeaderImageSrcSetUrl %>" alt="<%= projectData.ssHeaderImageAlt %>" class="project__technology__stories-header-img">
            <div class="project__technology__stories-paginator-wrapper">
              <div class="paginator__box paginator__box-left projects-disabled-paginator" id="stories-bck-project-<%=index + 1%>">
                  <svg xmlns="http://www.w3.org/2000/svg" width="33.384" height="25.521" viewBox="0 0 33.384 25.521" class="paginator__arrow">
                      <path id="thin_arrow-1-<%=index + 1%>" d="M8.527 30.913l10.938 10.981a1.755 1.755 0 002.46-2.5l-7.951-7.954h25.653a1.757 1.757 0 100-3.514H13.974l7.995-7.995a1.77 1.77 0 00-2.5-2.5L8.527 28.409a1.75 1.75 0 000 2.504z" data-name="Thin Arrow" transform="translate(-8 -16.9)"/>
                  </svg>
              </div>
              <div class="paginator__box paginator__box-right" id="stories-fwd-project-<%=index + 1%>">
                  <svg xmlns="http://www.w3.org/2000/svg" width="33.384" height="25.521" viewBox="0 0 33.384 25.521" class="paginator__arrow">
                      <path id="thin_arrow-2-<%=index + 1%>" d="M40.857 30.913L29.919 41.894a1.755 1.755 0 11-2.46-2.5l7.951-7.954H9.757a1.757 1.757 0 110-3.514H35.41l-7.995-7.995a1.77 1.77 0 112.5-2.5l10.942 10.978a1.75 1.75 0 010 2.504z" data-name="Thin Arrow" transform="translate(-8 -16.9)"/>
                  </svg>
              </div>
            </div>
          </header>
          <div class="project__technology__stories-info-wrapper" id="stories-project-<%=index + 1%>">
          <%_ projectData.stories.forEach((story, storyIndex) =>{ -%>
            <div class="project__technology__stories-info <%_ if(storyIndex === 0){ %> project__technology__stories-info-front <%_ }_%>" id="project-<%=index + 1%>-story-<%=storyIndex + 1%>">
              <div class="project__technology__stories-author">
                <header class="project__technology__stories-author-img-box">
                  <img src="<%= story.data.userImageUrl %>" srcset="<%= story.data.userImageSrcSetUrl %>" alt="<%= story.data.userName %>" class="project__technology__stories-author-img">
                </header>
                <h5 class="project__technology__stories-author-name"><%= story.data.userName %></h5>
                <p class="project__technology__stories-author-details"><%= story.data.userDesignation %></p>
              </div>
              <div class="project__technology__stories-text-wrapper">
                <div class="project__technology__stories-text-box">
                  <h5 class="project__technology__stories-text-heading"><%= story.data.heading %></h5>
                  <p class="project__technology__stories-text-info"><%= story.data.info %></p>
                </div>
                <img src="./assets/quotes.svg" alt='"' class="author__quote_img">
              </div>
            </div>
          <%_ }) _%>
          </div>
        </div>
      </section>
    <%_ }) _%>
        <div class="project__explore">
          <h3 class="project__explore-heading">Explore More Projects</h3>
          <div class="project__explore-links-wrapper">
          <%_ _projects.forEach((projectData, index) =>{ -%>
            <a href="#projNo=<%=index + 1%>" class="project__explore-links"><%= projectData.exploreName %></a>
          <%_ }) _%>
          </div>
        </div>
        <div class="project__cards__section">
          <div class="project__cards__wrapper" id="project__cards__wrapper">
          <%_ _projects.forEach((projectData, index) =>{ -%>
            <a class="project__card <%_ if(index < 3){ %> project__card--front <%_ }_%>" id="project__card-<%=index + 1%>" href="#projNo=<%=index + 1%>">
              <img src="<%= projectData.card.imageUrl %>" srcset="<%= projectData.card.imageSrcSetUrl %>" alt="<%= projectData.card.altName %>" class="project__card-header-img">
              <div class="project__card-body">
                <div class="project__card-info">
                  <span class="project__card-info-num"><%_ if(index < 9){ %> 0 <%_ }_%><%=index + 1%></span>
                  <div class="project__card-info-box">
                    <div class="project__card-info-client"><%= projectData.clientName %></div>
                    <div class="project__card-info-industry"><%= projectData.industry %></div>
                  </div>
                </div>
                <h6 class="project__card-heading"><%= projectData.projectName %></h6>
                <p class="project__card-text"><%= projectData.description %></p>
                <footer class="project__card-footer">
                  <div class="project__card-btn">
                      <div class="project__card-btn-text">Read more</div>
                      <span class="project__card-btn-svg-wrapper">
                          <svg xmlns="http://www.w3.org/2000/svg" width="33.384" height="14.8" viewBox="0 0 33.384 25.521" class="project__card-btn-svg">
                              <path id="prefix__Thin_Arrow-<%=index + 1%>" d="M40.857 30.913L29.919 41.894a1.755 1.755 0 11-2.46-2.5l7.951-7.954H9.757a1.757 1.757 0 110-3.514H35.41l-7.995-7.995a1.77 1.77 0 112.5-2.5l10.942 10.978a1.75 1.75 0 010 2.504z" data-name="Thin Arrow" transform="translate(-8 -16.9)"/>
                          </svg>
                      </span>
                  </div>
                </footer>
              </div>
            </a>
          <%_ }) _%>
          </div>
          <div class="project__cards-paginator-wrapper">
            <div class="paginator__box paginator__box-left projects-disabled-paginator" id="project-bck">
              <svg xmlns="http://www.w3.org/2000/svg" width="33.384" height="25.521" viewBox="0 0 33.384 25.521" class="paginator__arrow">
                <path id="thin_arrow-3" d="M8.527 30.913l10.938 10.981a1.755 1.755 0 002.46-2.5l-7.951-7.954h25.653a1.757 1.757 0 100-3.514H13.974l7.995-7.995a1.77 1.77 0 00-2.5-2.5L8.527 28.409a1.75 1.75 0 000 2.504z" data-name="Thin Arrow" transform="translate(-8 -16.9)"/>
              </svg>
            </div>
            <div class="paginator__box paginator__box-right" id="project-fwd">
              <svg xmlns="http://www.w3.org/2000/svg" width="33.384" height="25.521" viewBox="0 0 33.384 25.521" class="paginator__arrow">
                <path id="thin_arrow-4" d="M40.857 30.913L29.919 41.894a1.755 1.755 0 11-2.46-2.5l7.951-7.954H9.757a1.757 1.757 0 110-3.514H35.41l-7.995-7.995a1.77 1.77 0 112.5-2.5l10.942 10.978a1.75 1.75 0 010 2.504z" data-name="Thin Arrow" transform="translate(-8 -16.9)"/>
              </svg>
            </div>
          </div>
        </div>
    </main>
    <div class="floating-icon invisible" id="floating__icon">
      <svg xmlns="http://www.w3.org/2000/svg" width="30.822" height="30.822" viewBox="0 0 30.822 30.822" class="floating-icon-svg">
          <path id="prefix__XMLID_933_" d="M50.322 35.2a2.606 2.606 0 01-2.622 2.619H37.237V47.7a2.617 2.617 0 01-5.237 0v-9.881h-9.883a2.617 2.617 0 010-5.234H32V22.117a2.617 2.617 0 015.234 0v10.468H47.7a2.606 2.606 0 012.622 2.615z" transform="translate(-19.5 -19.5)"/>
      </svg>
    </div>
    <div class="site__footer-section" id="footer">
        <div class="footer__company__contacts">
            <div class="footer__company__contacts__img-box">
                <img src="./assets/logo.png" srcset="./assets/logo.png 1x, ./assets/logo@2x.png 2x" alt="Opscale Logo" class="footer__company__contacts__img">
            </div>
            <ul class="footer__company__contacts__list">
                <li class="footer__company__contacts__list-item">
                    <span class="company__contacts__svg-box">
                        <svg xmlns="http://www.w3.org/2000/svg" width="13" height="22" viewBox="0 0 13 22" class="company__contacts__svg">
                            <path id="prefix__cellphone-iphone_1_" d="M16 18H7V4h9m-4.5 18a1.5 1.5 0 111.5-1.5 1.5 1.5 0 01-1.5 1.5m4-21h-8A2.5 2.5 0 005 3.5v17A2.5 2.5 0 007.5 23h8a2.5 2.5 0 002.5-2.5v-17A2.5 2.5 0 0015.5 1z" data-name="cellphone-iphone (1)" transform="translate(-5 -1)" style="fill:#fff;opacity:.801"/>
                        </svg>
                    </span>
                    <span class="company__contacts__info">+91 7760830496</span>
                </li>
                <li class="footer__company__contacts__list-item">
